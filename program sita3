#include<stdio.h>
int main()
{
    char ch;
    printf("enter any letter:");
    scanf("%c",&ch);
    if(ch=='A'||ch=='a'||ch=='E'||ch=='e'||ch=='I'||ch=='i'||ch=='O'||ch=='o'||ch=='U'||ch=='u')
    {
       printf("the character of %c is a vowel",ch);
    }
    else
    {
        printf("the character of %c is a consonant",ch);
    }
}
